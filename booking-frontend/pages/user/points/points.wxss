.container {
  padding: 30rpx;
  background-color: #f7f7f7;
  min-height: 100vh;
}

.card {
  background-color: #fff;
  border-radius: 16rpx;
  padding: 30rpx;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.05);
}

.title {
  font-size: 34rpx;
  font-weight: 600;
  color: #333;
  margin-bottom: 30rpx;
  display: block;
}

.content {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 300rpx;
}

.placeholder {
  font-size: 28rpx;
  color: #999;
} 